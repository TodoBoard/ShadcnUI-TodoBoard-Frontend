  title: string;
  description: string;
  priority: string;
  due_date?: string;
  project_id: string;
}

export interface TodoUpdateSchema {
  title?: string;
  description?: string;
  status?: string;
  priority?: string;
  due_date?: string;
}

export interface TodoResponse extends Todo {
  message?: string;
}


